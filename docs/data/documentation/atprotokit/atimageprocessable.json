{"identifier":{"url":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATImageProcessable","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Provides a standardized approach to processing images for uploading to servers that interact"},{"type":"text","text":" "},{"type":"text","text":"with the AT Protocol."}],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"languages":["swift"],"tokens":[{"kind":"keyword","text":"protocol"},{"text":" ","kind":"text"},{"text":"ATImageProcessable","kind":"identifier"}],"platforms":["macOS"]}]},{"content":[{"level":2,"anchor":"overview","type":"heading","text":"Overview"},{"type":"paragraph","inlineContent":[{"type":"text","text":"This protocol aids in the conversion of images into an "},{"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ImageQuery","isActive":true,"type":"reference"},{"text":" (which is used to give","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"all of the necessary information about the image for the server), while ensuring that"},{"type":"text","text":" "},{"type":"text","text":"privacy-sensitive EXIF data is stripped from the images before upload."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Implementing "},{"type":"codeVoice","code":"ATImageProcessable"},{"type":"text","text":" allows for the customization of image processing tasks,"},{"text":" ","type":"text"},{"type":"text","text":"making it useful for developers working on platforms where standard image objects like"},{"text":" ","type":"text"},{"type":"codeVoice","code":"UIImage"},{"type":"text","text":" or "},{"type":"codeVoice","code":"NSImage"},{"type":"text","text":" are not available, such as in Linux environments."}]},{"type":"heading","text":"Methods","level":3,"anchor":"Methods"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"convertToImageQuery(image:altText:targetFileSize)"},{"type":"text","text":": Converts an image object into an"},{"text":" ","type":"text"},{"type":"codeVoice","code":"ImageQuery"},{"text":" instance. This instance encapsulates the image data along with metadata such as the file","type":"text"},{"type":"text","text":" "},{"type":"text","text":"name and alternative text, preparing it for upload. It also attempts to shrink the file size"},{"text":" ","type":"text"},{"type":"text","text":"down to an approprate amount."}]}]},{"content":[{"inlineContent":[{"type":"codeVoice","code":"stripMetadata(from:)"},{"type":"text","text":": Removes EXIF and GPS metadata from the provided image data."},{"text":" ","type":"text"},{"type":"text","text":"This is crucial for protecting privacy and reducing the size of the image data being transmitted."}],"type":"paragraph"}]}]},{"name":"Important","type":"aside","style":"important","content":[{"type":"paragraph","inlineContent":[{"text":"","type":"text"},{"type":"codeVoice","code":"stripMetadata(from:)"},{"text":" is an important method to create as, according to the","type":"text"},{"type":"text","text":" "},{"type":"text","text":"AT Protocol documentation, the protocol may be more strict about stripping metadata in the future."},{"text":"\n","type":"text"},{"type":"text","text":"\n"},{"type":"text","text":"Also, this should be an "},{"code":"internal","type":"codeVoice"},{"text":" method, as it will be part of","type":"text"},{"text":" ","type":"text"},{"code":"convertToImageQuery(image:altText:targetFileSize)","type":"codeVoice"},{"text":". It’s recommended that it’s called before","type":"text"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATImageProcessable\/convertToImageQuery(imagePath:altText:targetFileSize:)-2fma7"},{"text":" attempts to access the image.","type":"text"}]}]},{"level":3,"anchor":"Example","text":"Example","type":"heading"},{"type":"paragraph","inlineContent":[{"text":"Below is a sample implementation showcasing how to conform to ","type":"text"},{"code":"ATImageProcessable","type":"codeVoice"},{"type":"text","text":" for a"},{"type":"text","text":" "},{"text":"custom image type:","type":"text"}]},{"type":"codeListing","syntax":"swift","code":["class CustomImageProcessor: ATImageProcessable {","     func convertToImageQuery(imagePath: String, altText: String, targetFileSize: Int = 1_000_000) -> ImageQuery? {","         let atImage = stripMetadata(from: image)","         guard let customImage = atImage as? CustomImageType else {","             return nil","         }","         let imageData = customImage.convertToData() \/\/ Your method to convert the image to Data","         let fileName = \"customImage.jpg\" \/\/ Determine an appropriate file name","","         \/\/ Strip EXIF data if necessary","         let cleanImageData = stripEXIFData(from: imageData)","","         return ImageQuery(imageData: cleanImageData, fileName: fileName, altText: altText)","     }","","     func stripMetadata(from image: ATImage) -> Data {","         \/\/ Implementation to remove EXIF data from the image data.","         return data","     }","}",""]},{"level":3,"text":"Usage","anchor":"Usage","type":"heading"},{"inlineContent":[{"type":"text","text":"After implementing "},{"type":"codeVoice","code":"ATImageProcessable"},{"type":"text","text":", you can use your custom image processor to prepare"},{"text":" ","type":"text"},{"type":"text","text":"images for upload in a way that aligns with your application’s requirements:"}],"type":"paragraph"},{"type":"codeListing","code":["Task {","    let customImageProcessor = CustomImageProcessor()","    let image1 = customImageProcessor.convertToData(\"path\/to\/image\/cat.jpg\", withAltText: \"A cat, looking directly at the camera.\").stripEXIFData()","    let imageQueries = [image1]","","    await atProto.createPostRecord(text: \"Look at my cute cat!\", embed: .images(images: imageQueries))","}"],"syntax":"swift"}],"kind":"content"}],"schemaVersion":{"patch":0,"minor":3,"major":0},"variants":[{"traits":[{"interfaceLanguage":"swift"}],"paths":["\/documentation\/atprotokit\/atimageprocessable"]}],"topicSections":[{"identifiers":["doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATImageProcessable\/convertToImageQuery(imagePath:altText:targetFileSize:)-7bst7","doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATImageProcessable\/stripMetadata(from:)-4p2r3"],"title":"Instance Methods"}],"metadata":{"modules":[{"name":"ATProtoKit"}],"role":"symbol","externalID":"s:10ATProtoKit18ATImageProcessableP","fragments":[{"kind":"keyword","text":"protocol"},{"text":" ","kind":"text"},{"text":"ATImageProcessable","kind":"identifier"}],"title":"ATImageProcessable","roleHeading":"Protocol","navigatorTitle":[{"text":"ATImageProcessable","kind":"identifier"}],"symbolKind":"protocol"},"seeAlsoSections":[{"generated":true,"title":"Utilities","identifiers":["doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/APIClientService","doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/DateFormatting-PropertyWrapper"]}],"hierarchy":{"paths":[["doc:\/\/ATProtoKit\/documentation\/ATProtoKit"]]},"sections":[],"references":{"doc://ATProtoKit/documentation/ATProtoKit/ATImageProcessable/convertToImageQuery(imagePath:altText:targetFileSize:)-7bst7":{"title":"convertToImageQuery(imagePath:altText:targetFileSize:)","identifier":"doc:\/\/ATProtoKit\/documentation\/ATProtoKit\/ATImageProcessable\/convertToImageQuery(imagePath:altText:targetFileSize:)-7bst7","required":true,"kind":"symbol","role":"symbol","defaultImplementations":1,"url":"\/documentation\/atprotokit\/atimageprocessable\/converttoimagequery(imagepath:alttext:targetfilesize:)-7bst7","type":"topic","abstract":[{"text":"Processes an image to be suitable for the AT Protocol servers.","type":"text"}],"fragments":[{"text":"func","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"convertToImageQuery"},{"text":"(","kind":"text"},{"kind":"externalParam","text":"imagePath"},{"kind":"